    <ControlTemplate x:Key="ValidationErrorTemplate">
        <Grid Focusable="False">
            <Canvas x:Name="Cnv"
                Width="18"
                Height="18"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Background="{StaticResource Dur}"
                DataContext="{Binding ElementName=Adorner, Mode=OneWay}"
                RenderTransformOrigin="0.5,0.5"
                UseLayoutRounding="True">
                <Viewbox Width="18" Height="18">
                    <TextBlock Foreground="White">
                        <TextBlock.Text>
                            <MultiBinding Converter="{StaticResource ResourceLanguageStringConverter}">
                                <Binding Mode="OneWay" Source="STOP" />
                                <Binding Mode="OneWay" Path="Tag.Scanner.UiLanguage" />
                            </MultiBinding>
                        </TextBlock.Text>
                    </TextBlock>
                </Viewbox>
                <Canvas.ToolTip>
                    <ToolTip Background="{x:Null}"
                        BorderBrush="{x:Null}"
                        Foreground="{x:Null}">
                        <Border Padding="4"
                            Background="Red"
                            BorderBrush="White"
                            BorderThickness="1">
                            <TextBlock Foreground="White">
                                <TextBlock.Text>
                                    <MultiBinding Converter="{StaticResource ResourceLanguageStringConverter}">
                                        <Binding Mode="OneWay" Path="DataContext[0].ErrorContent" />
                                        <Binding Mode="OneWay" Path="Tag.Scanner.UiLanguage" />
                                    </MultiBinding>
                                </TextBlock.Text>
                            </TextBlock>
                        </Border>
                    </ToolTip>
                </Canvas.ToolTip>
            </Canvas>
            <AdornedElementPlaceholder x:Name="Adorner" Tag="{Binding RelativeSource={RelativeSource AncestorType=local:TwainCtrl}}" />
        </Grid>
    </ControlTemplate>
